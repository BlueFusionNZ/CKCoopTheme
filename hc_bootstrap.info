name = Harbour Co-op  - Bootstrap 3 Sub-theme
description = A Bootstrap 3 Sub-theme.
core = 7.x
base theme = bootstrap


;;;;;;;;;;;;;;;;;;;;;
;; Regions
;;;;;;;;;;;;;;;;;;;;;

regions[user]           = 'User Menu'
regions[top_search]     = 'Top Search'
regions[top_menu]       = 'Top Menu'
regions[highlighted]    = 'Highlighted'
regions[side_cart]    = 'Side Cart'
regions[help]           = 'Help'
regions[content]        = 'Content'
regions[sidebar_first]  = 'Primary'
regions[sidebar_second] = 'Secondary'
regions[footer]         = 'Footer'
regions[page_top]       = 'Page top'
regions[footer_first]    = 'Footer First'
regions[footer_second]    = 'Footer Second'
regions[footer_third]    = 'Footer Third'


;;;;;;;;;;;;;;;;;;;;;
;; Stylesheets
;;;;;;;;;;;;;;;;;;;;;

stylesheets[all][] = css/style.css


; ;;;;;;;;;;;;;;;;;;;;;
; ;; Local Scripts
; ;;;;;;;;;;;;;;;;;;;;;
scripts[] = js/omega_kickstart.js
scripts[] = js/jquery.hoverIntent.minified.js
scripts[] = js/main_menu_display.js
scripts[] = js/bootstrap-touchspin/bootstrap.touchspin.js
scripts[] = js/ready.js




